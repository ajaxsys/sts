          "_name": "footer1",
          "_customHTML": "<section class=\"footer1\" group=\"Footer\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" always-bottom global once=\"footer\" not-draggable>\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"checkbox\" title=\"Show Animation\" name=\"showAnimation\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Brand\" name=\"showBrand\" checked>\n        <select name=\"typeMenu\" title=\"Type Menu\" condition=\"showBrand\">\n            <option value=\"1\" selected>Logo Text</option>\n            <option value=\"2\">Logo Image</option>\n        </select>\n        <input type=\"range\" inline title=\"Width Logo\" name=\"widthLogo\" min=\"20\" max=\"100\" step=\"2\" value=\"60\" condition=\"showBrand && typeMenu == 2\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"range\" inline title=\"Menu Items 1\" name=\"menuItems\" min=\"0\" max=\"6\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Menu Items 2\" name=\"menuItems2\" min=\"0\" max=\"6\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Social Icons\" name=\"socIcons\" min=\"0\" max=\"4\" step=\"1\" value=\"4\">\n        <input type=\"color\" title=\"Icon Color\" value=\"#dbdbdb\" name=\"iconColor\" condition=\"socIcons > 0\">\n        <hr>\n        <input type=\"checkbox\" title=\"Show Bottom\" name=\"showBottom\" checked>\n        <input type=\"color\" title=\"Divider Color\" value=\"#000000\" name=\"dividerColor\" condition=\"showBottom\">\n        <input type=\"range\" inline title=\"Menu Bottom Items\" name=\"menuBottomItems\" min=\"0\" max=\"6\" step=\"1\" value=\"1\" condition=\"showBottom\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"../_images/background/backgraund__trend.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"animation__background\" mbr-if=\"showAnimation\">\n        <img src=\"../_images/figure/triangle_purpl_semitransp.svg\" class=\"triangle\" alt title>\n        <div class=\"circle\"></div>\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-lg-3 col-sm-6 col-12 wrap order-0\" mbr-if=\"showBrand\">\n               <p class=\"brand__text mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"typeMenu == 1\" data-app-selector=\".brand__text\">&nbsp;</p>\n                 <img class=\"logo__image\" mbr-if=\"typeMenu == 2\" src=\"../_images/logo2.png\" alt=\"Mobirise\" title>\n                <p class=\"brand__email mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".brand__email\"><a href=\"mailto:info@shibuyatravel.jp\" class=\"text-secondary text-primary\"><b>info@shibuyatravel.jp</b></a></p>\n                <p class=\"brand__phone mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".brand__phone\"><a href=\"tel: +81-03-6419-1061\" class=\"text-warning text-primary\">&nbsp;+(81)03-6419-1061</a></p>\n            </div>\n            <div class=\"col-lg-3 col-sm-6 col-12 wrap\">\n                <h4 class=\"mbr-fonts-style mbr-regular footer__title\" mbr-if=\"showTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".footer__title\">&nbsp;</h4>\n                <div class=\"mbr-list\" mbr-list mbr-list-grow=\"menuItems\" mbr-if=\"menuItems > 0\">\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__items\"><a href=\"service.html\" class=\"text-primary\">サービス</a><br><a href=\"about.html\" class=\"text-primary\">会社概要</a></p>\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__items\"><a href=\"recruit.html\" class=\"text-primary\">人材募集</a></p>\n                    \n                </div>\n            </div>\n            <div class=\"col-lg-3 col-sm-6 col-12 wrap\">\n                <h4 class=\"mbr-fonts-style mbr-regular footer__title\" mbr-if=\"showTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".footer__title\">&nbsp;</h4>\n               <div class=\"mbr-list\" mbr-list mbr-list-grow=\"menuItems2\" mbr-if=\"menuItems2 > 0\">\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__items\">&nbsp;</p>\n                    \n                    \n                </div>\n            </div>\n            <div class=\"col-lg-3 col-sm-6 col-12 wrap\">\n                <h4 class=\"mbr-fonts-style mbr-regular footer__title\" mbr-if=\"showTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".footer__title\">&nbsp;</h4>\n                <p class=\"footer__adress mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__adress\">Follow us</p>\n                    <div c